{
  "name": "exam",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=text cross-env NODE_ENV=test mocha ./test/*",
    "lint-all": "eslint './**'",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --seeders-path ./seeders/test",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "start": "node index.js",
    "dev": "nodemon -L index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.4",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "mysql2": "^2.3.3",
    "nodejs": "^0.0.0",
    "request": "^2.88.2",
    "sequelize": "^6.13.0",
    "supertest": "^6.2.1"
  },
  "devDependencies": {
    "body-parser": "^1.19.1",
    "eslint": "^8.7.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0"
  }
}
